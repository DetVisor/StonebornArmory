      <AccuracyLong>0.55</AccuracyLong>
      <RangedWeapon_Cooldown>2</RangedWeapon_Cooldown>
    </statBases>

    <costList>
      <Steel>65</Steel>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>

    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
